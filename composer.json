{
    "name": "reedware/laravel-api",
    "description": "Adds configurable web api connections to your Laravel application.",
    "keywords": [
        "laravel", "api", "connection"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\LaravelApi\\": "src/"
        }
    },
    "autoload-dev": {},
    "suggest": {},
    "conflict": {},
    "config": {},
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "laravel": {
            "providers": [
                "Reedware\\LaravelApi\\ApiServiceProvider"
            ],
            "aliases": {
                "Api": "Reedware\\LaravelApi\\Facade"
            }
        }
    }
}
